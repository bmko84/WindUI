-- HITBOX
CheatTab:Button({
    Title = "Hitbox",
    Description = "Enable Hitbox Script",
    Callback = function()
        loadstring(game:HttpGet("https://pastefy.app/ItfO0tdg/raw"))()
    end
})

-- NOCLIP
CheatTab:Toggle({
    Title = "‡πÄ‡∏î‡∏¥‡∏ô‡∏ó‡∏∞‡∏•‡∏∏ (NoClip)",
    Default = false,
    Callback = function(state)
        noclipEnabled = state
    end
})

RunService.Stepped:Connect(function()
    if noclipEnabled and LocalPlayer.Character then
        for _, part in ipairs(LocalPlayer.Character:GetDescendants()) do
            if part:IsA("BasePart") and part.CanCollide then
                part.CanCollide = false
            end
        end
    end
end)

-- Small Toggle Shift Lock (‡∏£‡∏ß‡∏°‡πÄ‡∏Ç‡πâ‡∏≤‡πÅ‡∏ó‡πá‡∏ö CheatTab)
repeat task.wait() until game:IsLoaded() and LocalPlayer
local camera = workspace.CurrentCamera

local ScreenGui = Instance.new("ScreenGui")
ScreenGui.Name = "SmallShiftLockGUI"
ScreenGui.ResetOnSpawn = false
ScreenGui.Parent = LocalPlayer:WaitForChild("PlayerGui")

local ToggleButton = Instance.new("TextButton")
ToggleButton.Size = UDim2.new(0, 30, 0, 30)
ToggleButton.Position = UDim2.new(0, 20, 0, 20)
ToggleButton.Text = ""
ToggleButton.BackgroundColor3 = Color3.fromRGB(0, 170, 255)
ToggleButton.BorderSizePixel = 0
ToggleButton.AutoButtonColor = true
ToggleButton.Parent = ScreenGui

local CenterFrame = Instance.new("Frame")
CenterFrame.Size = UDim2.new(0, 8, 0, 8)
CenterFrame.Position = UDim2.new(0.5, 0, 0.5, 0)
CenterFrame.AnchorPoint = Vector2.new(0.5, 0.5)
CenterFrame.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
CenterFrame.BorderSizePixel = 0
CenterFrame.Visible = false
CenterFrame.Parent = ScreenGui

local lines = {}
local offsets = {UDim2.new(0.5,0,0.5,-8),UDim2.new(0.5,0,0.5,8),UDim2.new(0.5,-8,0.5,0),UDim2.new(0.5,8,0.5,0)}
local sizes = {UDim2.new(0,2,0,4),UDim2.new(0,2,0,4),UDim2.new(0,4,0,2),UDim2.new(0,4,0,2)}
for i=1,4 do
    local line = Instance.new("Frame")
    line.Size = sizes[i]
    line.Position = offsets[i]
    line.AnchorPoint = Vector2.new(0.5,0.5)
    line.BackgroundColor3 = Color3.fromRGB(255,255,255)
    line.BorderSizePixel = 0
    line.Visible = false
    line.Parent = ScreenGui
    table.insert(lines,line)
end

local isEnabled = false
local function toggleShiftLock()
    isEnabled = not isEnabled
    CenterFrame.Visible = isEnabled
    for _, line in pairs(lines) do line.Visible = isEnabled end
end
ToggleButton.MouseButton1Click:Connect(toggleShiftLock)

RunService.RenderStepped:Connect(function()
    if isEnabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
        local hrp = LocalPlayer.Character.HumanoidRootPart
        local screenCenter = Vector2.new(camera.ViewportSize.X/2,camera.ViewportSize.Y/2)
        local ray = camera:ViewportPointToRay(screenCenter.X,screenCenter.Y)
        local lookAt = ray.Origin + ray.Direction*100
        hrp.CFrame = CFrame.new(hrp.Position,Vector3.new(lookAt.X,hrp.Position.Y,lookAt.Z))
    end
end)

