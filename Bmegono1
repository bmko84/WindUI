-- CHEAT TAB ("‡πÇ‡∏Å‡∏á‡πÇ‡∏´‡∏î‡πÜ")
-- =========================
-- GOD MODE
CheatTab:Toggle({
    Title = "God Mode",
    Default = false,
    Callback = function(state)
        godEnabled = state
        if godEnabled and LocalPlayer.Character and LocalPlayer.Character:FindFirstChildOfClass("Humanoid") then
            LocalPlayer.Character:FindFirstChildOfClass("Humanoid").Name = "GodHumanoid"
        end
    end
})

